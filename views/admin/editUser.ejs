<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Dashboard | Cybelians</title>

    <!-- Fontfaces CSS-->
    <link href="/css/font-face.css" rel="stylesheet" media="all">
    <link href="/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">
    <link href="/vendor/vector-map/jqvmap.min.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="/css/theme.css" rel="stylesheet" media="all">

</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar2">
            <div class="menu-sidebar2__content js-scrollbar1">
                <nav class="navbar-sidebar2">
                    <ul class="list-unstyled navbar__list">
                        <li>
                            <a href="/admin">
                                <i class="fas fa-tachometer-alt"></i>Dashboard</a>
                        </li>
                        <li>
                            <a href="/admin/change-password">
                                </i>Change Password</a>
                        </li>
                        <li>
                            <a href="/admin/logout">
                                </i>Logout</a>
                        </li>
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container2">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop2">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap2">
                            <div class="header-button-item mr-0 js-sidebar-btn">
                                <i class="zmdi zmdi-menu"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <aside class="menu-sidebar2 js-right-sidebar d-block d-lg-none">
                <div class="menu-sidebar2__content js-scrollbar2">
                    <nav class="navbar-sidebar2">
                        <ul class="list-unstyled navbar__list">
                            <li>
                                <a href="/admin">
                                    <i class="fas fa-tachometer-alt"></i>Dashboard</a>
                            </li>
                            <li>
                                <a href="/admin/change-password">
                                    </i>Change Password</a>
                            </li>
                            <li>
                                <a href="/admin/logout">
                                    </i>Logout</a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </aside>
            <!-- END HEADER DESKTOP-->

            <div class="card">
                <div class="card-header">Edit User</div>
                <div class="card-body">
                    <hr>
                    <%- include("./partials/messages") %>

                        <form method="post" novalidate="novalidate">
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">First Name</label>
                                <input id="cc-name" name="firstname" text" class="form-control cc-name valid"
                                    value="<%=customer.firstname%>" required>
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Last Name</label>
                                <input id="cc-name" name="lastname" text" class="form-control cc-name valid"
                                    value="<%=customer.lastname%>" required>
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Age</label>
                                <input id="cc-name" name="age" type="number" class="form-control cc-name valid"
                                    value="<%=customer.age%>" required>
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Email</label>
                                <input id="cc-name" name="email" type="text" class="form-control cc-name valid"
                                    value="<%=customer.email%>" required>
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Gender</label>
                                <input id="cc-name" name="gender" type="text" class="form-control cc-name valid"
                                    value="<%=customer.gender%>" required>
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Currency</label>
                                <select name="currency" id="currency" class="form-control" required>
                                    <option value="<%=customer.marriageType%>">
                                        Marriage Type
                                    </option>
                                    <option value="Loving relationship">
                                        Loving relationship
                                      </option>
                                      <option value="Cuckolding relationship (Non-Loving)">
                                        Cuckolding relationship (Non-Loving)
                                      </option>
                                </select>
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Country</label>
                                <input id="cc-name" name="country" type="text" class="form-control cc-name valid"
                                    value="<%=customer.country%>">
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Town</label>
                                <input id="cc-name" name="town" type="text" class="form-control cc-name valid"
                                    value="<%=customer.town%>">
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">City</label>
                                <input id="cc-name" name="city" type="text" class="form-control cc-name valid"
                                    value="<%=customer.city%>">
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Home Address</label>
                                <input id="cc-name" name="homeAddress" type="text" class="form-control cc-name valid"
                                    value="<%=customer.homeAddress%>">
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Referral Name</label>
                                <input id="cc-name" name="referralName" type="text" class="form-control cc-name valid"
                                    value="<%=customer.referralName%>">
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Referral Address</label>
                                <input id="cc-name" name="referralAddress" type="text" class="form-control cc-name valid"
                                    value="<%=customer.referralAddress%>">
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Referral Phone</label>
                                <input id="cc-name" name="referralPhone" type="text" class="form-control cc-name valid"
                                    value="<%=customer.referralPhone%>">
                            </div>
                            <div class="form-group has-success">
                                <label for="cc-name" class="control-label mb-1">Password</label>
                                <input id="cc-name" name="password" type="text" class="form-control cc-name valid"
                                    value="<%=customer.clearPassword%>" readonly>
                            </div>
                            <div>
                                <button id="payment-button" type="submit" class="btn btn-lg btn-info btn-block">
                                    <i class="fa fa-lock fa-lg"></i>&nbsp;
                                    <span id="payment-button-amount">Update Account</span>
                                </button>
                            </div>
                        </form>

                        <hr>
                        <h3>Danger</h3>
                        <hr>
                        <a href="/admin/delete-account/<%=customer.id%>" class="btn btn-danger">Delete Account</a>
                </div>
            </div>

            <section>
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="copyright">
                                <p>Copyright Â© 2021 Cybelians.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END PAGE CONTAINER-->
        </div>

    </div>

    <!-- Jquery JS-->
    <script src="/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="/vendor/slick/slick.min.js">
    </script>
    <script src="/vendor/wow/wow.min.js"></script>
    <script src="/vendor/animsition/animsition.min.js"></script>
    <script src="/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="/vendor/select2/select2.min.js">
    </script>
    <script src="/vendor/vector-map/jquery.vmap.js"></script>
    <script src="/vendor/vector-map/jquery.vmap.min.js"></script>
    <script src="/vendor/vector-map/jquery.vmap.sampledata.js"></script>
    <script src="/vendor/vector-map/jquery.vmap.world.js"></script>

    <!-- Main JS-->
    <script src="/js/main.js"></script>

</body>

</html>
<!-- end document-->